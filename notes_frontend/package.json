{
  "name": "reactnative",
  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "lint": "eslint .",
    "prebuild:android": "expo prebuild --platform android",
    "build": "npm run ci:lint && node -e \"const fs=require('fs');if(fs.existsSync('android/gradlew')){console.log('Using native build via build:native');process.exit(0);}else{console.log('Expo managed app: skipping Gradle.');process.exit(0);}\"",
    "build:native": "npm run prebuild:android && cd android && ./gradlew assembleDebug",
    "ci:lint": "eslint . --max-warnings=0",
    "ci:build": "node -e \"const fs=require('fs');if(fs.existsSync('android/gradlew')){console.log('Running Gradle build...');process.exit(0);}else{console.log('Skipping Gradle: android/ not found (Expo managed).');process.exit(0);}\""
  },
  "dependencies": {
    "@expo/metro-runtime": "~5.0.4",
    "@react-navigation/bottom-tabs": "^7.2.0",
    "@react-navigation/native": "^7.0.14",
    "@react-navigation/native-stack": "^7.1.10",
    "expo": "~53.0.20",
    "expo-status-bar": "~2.2.3",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-native": "0.79.5",
    "react-native-gesture-handler": "~2.20.2",
    "react-native-safe-area-context": "4.12.0",
    "react-native-screens": "~4.4.0",
    "react-native-vector-icons": "^10.2.0",
    "@expo/vector-icons": "^14.0.3",
    "react-native-reanimated": "~3.16.1",
    "@react-native-async-storage/async-storage": "1.23.1",
    "react-native-web": "^0.20.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@types/react": "~19.0.10",
    "eslint": "^9.33.0",
    "eslint-plugin-react": "^7.37.5",
    "globals": "^16.3.0",
    "jiti": "^2.5.1",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.40.0"
  },
  "private": true
}
